{% extends "template.njk" %}

{% block main %}
<div class="faq-page ">

  {% include "_header-1.njk" %}

  <div class="page-banner">
    <div class="container">
      <div class="row justify-content-center text-center">
        <div class="col-lg-8">
          <div class="banner-content">
            <h2 class="title">
              Frequently Asked Question
            </h2>
            <p>Create custom landing pages with anya that converts.</p>
          </div>
        </div>
      </div>
    </div>
  </div>
<div class="contact-section pb-md--120 pb--50">
 <div class="container">
  <div class="row">
    <div class="col-lg-4 col-md-4 mb--30 mb-md--0">
      <ul class="nav nav-tabs faq-tab" id="faq-tab" role="tablist">
        <li class="nav-item">
          <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">General</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Accounts</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" id="sales-tab" data-toggle="tab" href="#sales" role="tab" aria-controls="sales" aria-selected="false">Sales</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" id="support-tab" data-toggle="tab" href="#support" role="tab" aria-controls="support" aria-selected="false">Support</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" id="licence-tab" data-toggle="tab" href="#licence" role="tab" aria-controls="licence" aria-selected="false">License</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" id="refund-tab" data-toggle="tab" href="#refund" role="tab" aria-controls="refund" aria-selected="false">Refund</a>
        </li>
      </ul>
    </div>
    <div class="col-lg-8 col-md-8">
      <div class="tab-content faq-black faq-wrapper" id="faqTabContent">
        <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
          {% include "_single-faqs.njk" %}
        </div>
        <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
          {% include "_single-faqs.njk" %}
        </div>
        <div class="tab-pane fade" id="sales" role="tabpanel" aria-labelledby="sales-tab">
          {% include "_single-faqs.njk" %}
        </div>
        <div class="tab-pane fade" id="support" role="tabpanel" aria-labelledby="support-tab">
          {% include "_single-faqs.njk" %}
        </div>
        <div class="tab-pane fade" id="licence" role="tabpanel" aria-labelledby="licence-tab">
          {% include "_single-faqs.njk" %}
        </div>
        <div class="tab-pane fade" id="refund" role="tabpanel" aria-labelledby="refund-tab">
          {% include "_single-faqs.njk" %}
        </div>
      </div>
    </div>
  </div>
 </div>
</div>

  <!-- Footer section -->
  {% include "_footer-1.njk" %}

</div>
{% endblock %}