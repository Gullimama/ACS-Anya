{% extends "template.njk" %}

{% block main %}
<div class="blog-page">

  {% include "_header-1.njk" %}
  
  <!-- Page Banner Area -->
  <div class="page-banner">
    <div class="container">
      <div class="row justify-content-center text-center">
        <div class="col-lg-6">
          <div class="banner-content">
            <h2 class="title">
              Anya Blogs
            </h2>
            <p>We stay on top of the latest in IT and share that with you </p>
          </div>
        </div>
      </div>
    </div>
  </div>

  {% set blog_post = [
    { 
    
      image: "./image/jpeg/blog-post--img.jpg", 
      date: "Jan 14, 2020",
      title: "How did we get 1M+ visitors in 30 days without anything!",
      para:"Throwing consider dwelling bachelor joy her proposal laughter. Raptures returned disposed one entirely her men ham. "
  
    },
    { 
    
      image: "./image/jpeg/blog-post--img-2.jpg", 
      date: "Jan 14, 2020",
      title: "How did we get 1M+ visitors in 30 days without anything!",
      para:"Throwing consider dwelling bachelor joy her proposal laughter. Raptures returned disposed one entirely her men ham. "
  
    },
    { 
    
      image: "./image/jpeg/blog-post--img-3.jpg", 
      date: "Jan 14, 2020",
      title: "How did we get 1M+ visitors in 30 days without anything!",
      para:"Throwing consider dwelling bachelor joy her proposal laughter. Raptures returned disposed one entirely her men ham. "
  
    },
    { 
    
      image: "./image/jpeg/blog-post--img-4.jpg", 
      date: "Jan 14, 2020",
      title: "How did we get 1M+ visitors in 30 days without anything!",
      para:"Throwing consider dwelling bachelor joy her proposal laughter. Raptures returned disposed one entirely her men ham. "
  
    },
    { 
    
      image: "./image/jpeg/blog-post--img-5.jpg", 
      date: "Jan 14, 2020",
      title: "How did we get 1M+ visitors in 30 days without anything!",
      para:"Throwing consider dwelling bachelor joy her proposal laughter. Raptures returned disposed one entirely her men ham. "
  
    },
    { 
    
      image: "./image/jpeg/blog-post--img-6.jpg", 
      date: "Jan 14, 2020",
      title: "How did we get 1M+ visitors in 30 days without anything!",
      para:"Throwing consider dwelling bachelor joy her proposal laughter. Raptures returned disposed one entirely her men ham. "
  
    }
    ] 
  %}
  <div class="blog-post-area mb--20 mb-md--0">
    <div class="container">
      <div class="row">
        {% for post in blog_post %}
          <div class="col-xl-4 col-lg-6 col-md-6 mb--40">
            <div class="blog-card">
              <div class="blog-card_img">
                <img src="{{ post.image }}" alt="">
              </div>
              <div class="blog-card_texts">
                <span class="post-date">{{ post.date }}</span>
                <h3 class="post-title">
                  {{ post.title }}
                </h3>
                <p>{{ post.para }}</p>
                <a href="#" class="link-to-more">Continue Reading</a>
              </div>
            </div>
          </div>
        {% endfor %}
      </div>

      <div class="anya-blog-pagination pt--20">
        <ul class="pagination-list justify-content-center">
          <li><a href="#"><i class="fas fa-chevron-left"></i></a></li>
          <li><a href="#">1</a></li>
          <li><a href="#">2</a></li>
          <li><a href="#">3</a></li>
          <li class="exerpt">...</li>
          <li><a href="#">9</a></li>
          <li><a href="#"><i class="fas fa-chevron-right"></i></a></li>
        </ul>
      </div>
    </div>
  </div>

  <!-- CTA section -->
  <div class="omga-08__cta-section section-padding">
    <div class="shape-1" data-aos="fade-right" data-aos-duration="400" data-aos-once="true">
      <img src="./image/png/l1-cta-oval.png" alt="">
    </div>
    <div class="shape-2">
      <img src="./image/svg/l1-curve-cta.svg" alt="">
    </div>
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-lg-7 col-xl-6">
          <div class="section-title text-center" data-aos="zoom-out" data-aos-duration="500" data-aos-once="true">
            <h2 class="title">Want to join?</h2>
            <p>Are you interested to join our team? Check out our job openings and apply on your suitable role.</p>
            <div class="cta-btn">
              <a class="btn hvr-bounce-to-left" href="#">See opening positions</a>
            </div>
          </div>
        </div>
      </div>

    </div>
  </div>
 
  <!-- Footer section -->
  {% include "_inner-page-footer.njk" %}

</div>
{% endblock %}